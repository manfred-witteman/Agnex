{
  "manifest_version": 2,
  "name": "Agnes",
  "description": "The Easy Web Archiver",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
      {
        "matches": ["https://archive.is/*"],
        "css": ["archivekiller.css"]
      }
    ],
  "icons": {
          "48": "images/agnes@48.png",
          "96": "images/agnes@96.png",
          "128": "images/agnes@128.png",
          "256": "images/agnes@256.png",
          "512": "images/agnes@512.png"
      },
      "browser_action": {
          "default_icon": {
              "16": "images/agnes@16.png",
              "19": "images/agnes@19.png",
              "32": "images/agnes@32.png",
              "38": "images/agnes@38.png",
              "48": "images/agnes@48.png",
              "72": "images/agnes@72.png"
          }
      }
}
